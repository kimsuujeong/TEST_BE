{
  "family": "spring-task-def",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "spring-container",
      "image": "REPLACEME",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::339712949820:role/ecsTaskExecutionRole"
}
